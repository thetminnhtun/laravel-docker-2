{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "none"
        },
        "ordered_imports": {
            "case_sensitive": false,
            "imports_order": null,
            "sort_algorithm": "alpha"
        },
        "not_operator_with_successor_space": true,
        "single_line_comment_spacing": true,
        "nullable_type_declaration": false,
        "method_chaining_indentation": false
    }
}
